               <font>
                  <Font size="48.0" />
               </font>
            </Label>
            <Button fx:id="boonVolver" mnemonicParsing="false" onAction="#volver" text="Volver">
               <padding>
                  <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
               </padding>
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <BorderPane.margin>
